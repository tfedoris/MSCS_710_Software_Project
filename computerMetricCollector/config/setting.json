{
  "log_level": "DEBUG",
  "log_file": "MetricCollector.log",
  "log_rotate_time": "midnight",
  "log_backup_cnt": 7,
  "date_time_format": "yyyy-mm-dd HH:MM:SS",
  "sleep_time_sec": 60,
  "db_uri": "",
  "all_metrics": {
    "process": {
      "sql": "",
      "metrics": [
        "pid", "name", "status", "startTime", "cpuUserTime", "cpuKernelTime",
        "cpuPercent", "memoryPercentUsedByte", "memoryPhysicalUsedByte",
        "memoryVirtualUsedByte", "memoryUniqueUsedByte", "memoryNonPrivateByte",
        "memoryPageFault", "ioReadCnt", "ioReadBytes", "ioWriteCnt",
        "ioWriteBytes", "threadNum"
      ]
    },
    "memory": {
      "sql": "",
      "metrics": [
        "entry_datetime", "mem_total", "mem_available", "mem_used", "swap_total",
      ]
    },
    "cpu": {
      "sql": "",
      "metrics": [
        "entry_datetime", "brand", "vendor", "arch", "bits", "hz_advertise",
        "hz_actual", "count"
      ]
    }
  }
}