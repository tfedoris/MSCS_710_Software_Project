{
  "log_level": "DEBUG",
  "log_file": "MetricCollector.log",
  "log_rotate_time": "midnight",
  "log_backup_cnt": 7,
  "date_time_format": "%Y-%m-%d %H:%M:%S",
  "sleep_time_sec": 60,
  "local_store_dir": "\\local_data\\",
  "public_key_url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/get-public-key",
  "register_url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-map-user-machine",
  "to_store_local": false,
  "encryption_key_file": "\\crypto\\ppk\\public.pem",
  "collectors": {
    "ComputerMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-client-machine",
      "metrics_to_encrypt": [
        "machine_name",
        "system_name",
        "version",
        "machine_type"
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "machine_name",
        "system_name",
        "version",
        "machine_type",
        "nonce",
        "session_key"
      ]
    },
    "CPUMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-cpu-metrics",
      "metrics_to_encrypt": [
        "brand",
        "vendor",
        "architecture",
        "bits"
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "brand",
        "vendor",
        "architecture",
        "bits",
        "hz_advertise",
        "hz_actual",
        "core_count",
        "nonce",
        "session_key"
      ]
    },
    "DiskMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-disk-metrics",
      "metrics_to_encrypt": [
        "disk_name"
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "disk_name",
        "total_bytes",
        "free_bytes",
        "used_bytes",
        "percent",
        "nonce",
        "session_key"
      ]
    },
    "DiskIOMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-disk-io-metrics",
      "metrics_to_encrypt": [
        "disk_name",
        "time_read_in_milli",
        "time_write_in_milli"
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "disk_name",
        "count_read",
        "count_write",
        "bytes_read",
        "bytes_write",
        "time_read_in_milli",
        "time_write_in_milli",
        "nonce",
        "session_key"
      ]
    },
    "MemoryMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-memory-metrics",
      "metrics_to_encrypt": [
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "memory_total",
        "memory_available",
        "memory_used",
        "memory_used_percent",
        "swap_total",
        "swap_free",
        "swap_used",
        "swap_percent",
        "swap_byte_in",
        "swap_byte_out",
        "nonce",
        "session_key"
      ]
    },
    "NetworkMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-network-metrics",
      "metrics_to_encrypt": [
        "network_interface"
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "network_interface",
        "bytes_send",
        "bytes_receive",
        "error_bytes_receive",
        "error_bytes_send",
        "packet_sent",
        "packet_receive",
        "packet_receive_drop",
        "packet_send_drop",
        "nonce",
        "session_key"
      ]
    },
    "ProcessMetrics": {
      "url": "https://ytp3g6j58c.execute-api.us-east-2.amazonaws.com/test/insert-processes-metrics",
      "metrics_to_encrypt": [
        "pid",
        "name",
        "start_time",
        "start_user",
        "cpu_user_time",
        "cpu_kernel_time",
        "memory_page_fault",
        "io_read_count",
        "io_write_count"
      ],
      "metrics": [
        "machine_id",
        "entry_time",
        "pid",
        "name",
        "start_time",
        "start_user",
        "process_status",
        "cpu_user_time",
        "cpu_kernel_time",
        "cpu_percent",
        "memory_percent_used_byte",
        "memory_physical_used_byte",
        "memory_virtual_used_byte",
        "memory_unique_used_byte",
        "memory_page_fault",
        "io_read_count",
        "io_read_bytes",
        "io_write_count",
        "io_write_bytes",
        "thread_num",
        "nonce",
        "session_key"
      ]
    }
  }
}