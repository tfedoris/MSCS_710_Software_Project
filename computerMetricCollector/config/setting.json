{
  "log_level": "DEBUG",
  "log_file": "MetricCollector.log",
  "log_rotate_time": "midnight",
  "log_backup_cnt": 7,
  "date_time_format": "%Y-%m-%d %H:%M:%S",
  "sleep_time_sec": 60,
  "local_store_dir": "/data/xfer/",
  "to_store_local": true,
  "encryption_path": "computerMetricCollector\\dataEncryption\\ppk",
  "database": {
    "type": "mysql",
    "host":"",
    "user": "",
    "db_name": "",
    "port": 3306
  },
  "collectors": {
    "ComputerMetrics": {
      "table": "",
      "metrics": [
        "MachineID",
        "MachineName",
        "System",
        "Version",
        "MachineType"
      ]
    },
    "CPUMetrics": {
      "table": "",
      "metrics": [
        "MachineId",
        "EntryDatetime",
        "Brand",
        "Vendor",
        "Arch",
        "Bits",
        "HZAdvertise",
        "HZActual",
        "Count"
      ]
    },
    "DiskMetrics": {
      "table": "",
      "metrics": [
        "MachineID",
        "DiskName",
        "EntryDatetime",
        "Total",
        "Free",
        "Used",
        "Percent"
      ]
    },
    "DiskIOMetrics": {
      "table": "",
      "metrics": [
        "MachineId",
        "DiskName",
        "EntryDatetime",
        "CountRead",
        "CountWrite",
        "BytesRead",
        "BytesWrite",
        "TimeReadInMilli",
        "TimeWriteInMilli"
      ]
    },
    "MemoryMetrics": {
      "table": "",
      "metrics": [
        "MachineId",
        "EntryDatetime",
        "MemoryTotal",
        "MemoryAvailable",
        "MemoryUsed",
        "MemoryUsedPercent",
        "SwapTotal",
        "SwapFree",
        "SwapUsed",
        "SwapPercent",
        "SwapBytesIn",
        "SwapBytesOut"
      ]
    },
    "NetworkMetrics": {
      "table": "",
      "metrics": [
        "MachineId",
        "EntryDatetime",
        "NetworkInterface",
        "ByteSend",
        "ByteReceived",
        "ErrorByteReceived",
        "ErrorByteSend",
        "PacketSend",
        "PacketReceived",
        "PacketReceivedDrop",
        "PacketSendDrop"
      ]
    },
    "ProcessMetrics": {
      "table": "",
      "metrics": [
        "MachineId",
        "Pid",
        "Name",
        "StartTime",
        "User",
        "EntryDatetime",
        "Status",
        "CPUUserTime",
        "CPUKernelTime",
        "CPUPercent",
        "MemoryPercentUsedByte",
        "MemoryPhysicalUsedByte",
        "MemoryVirtualUsedByte",
        "MemoryUniqueUsedByte",
        "MemoryPageFault",
        "IOReadCnt",
        "IOReadBytes",
        "IOWriteCnt",
        "IOWriteBytes",
        "ThreadNum"
      ]
    }
  }
}