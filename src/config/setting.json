{
  "log_level": "DEBUG",
  "log_file": "MetricCollector.log",
  "log_rotate_time": "midnight",
  "log_backup_cnt": 7,
  "sleep_time_sec": 60,
  "db_uri": "",
  "all_metrics": {
    "process_metrics": {
      "sql": "",
      "metrics": [
        "pid",
        "name",
        "status",
        "startTime",
        "cpuUserTime",
        "cpuKernelTime",
        "cpuPercent",
        "memoryPercentUsedByte",
        "memoryPhysicalUsedByte",
        "memoryVirtualUsedByte",
        "memoryUniqueUsedByte",
        "memoryNonPrivateByte",
        "memoryPageFault",
        "ioReadCnt",
        "ioReadBytes",
        "ioWriteCnt",
        "ioWriteBytes",
        "threadNum"
      ]
    }
  }
}